     "execution_count": 70,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "matrix = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]\n",
    "matrix[0]  # first row"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 71,
   "id": "d61b9646",
   "metadata": {},
   "outputs": [
    {
     "data": {
